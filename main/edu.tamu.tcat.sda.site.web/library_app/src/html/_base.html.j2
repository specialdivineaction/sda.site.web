<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
   <meta charset="utf-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <title>{% block title %}Special Divine Action{% endblock %}</title>
   <meta name="description" content="{% block meta_description %}{%endblock%}">
   <meta name="viewport" content="width=device-width, initial-scale=1">

   {% set baseUrl = baseUrl | default('') %}
   {% block head %}{% endblock %}
   {% block stylesheets %}{% endblock %}
   <script src="{{ baseUrl }}/js/modernizr.js"></script>
   <noscript>
      <link rel="stylesheet" href="{{ baseUrl }}/assets/css/noscript.css">
   </noscript>
</head>

<body>
   <!--[if lt IE 9]>
      <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
   <![endif]-->

   {% block body %}{% endblock %}

   {% block javascripts %}
      <script src="{{ baseUrl }}/js/vendors.js"></script>
      <!--[if lt IE 9]>
         <script src="{{ baseUrl }}/js/vendors-ie.js"></script>
      <![endif]-->
   {% endblock %}
</body>
</html>
